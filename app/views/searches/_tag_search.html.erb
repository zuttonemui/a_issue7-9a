<% if user_signed_in? %>
  <%= form_with url: search_path, local: true, method: :get do |f| %>
    <%= f.text_field :content %>
    <%= f.hidden_field :model, :value => "tag" %>
    <%= f.hidden_field :method, :value => "perfect" %>
    <%= f.submit "tag_search", class: "btn btn-info btn-sm" %>
  <% end %>
<% end%>